{
  "name": "Sombrio",
  "version": "1.0.0",
  "main": "src-main/index.js",
  "license": "MIT",
  "scripts": {
    "build": "sagui build",
    "dist": "sagui dist",
    "format": "sagui format",
    "package": "yarn dist && ./node_modules/.bin/asar pack dist dist/app.asar",
    "start": "sagui develop --port 3000",
    "start-app": "NODE_ENV=development electron .",
    "start-app:prod": "NODE_ENV=production electron .",
    "test": "sagui test",
    "test:lint": "sagui test:lint",
    "test:typecheck": "sagui test:typecheck",
    "test:unit": "sagui test:unit --coverage",
    "test:unit:watch": "sagui test:unit --watch"
  },
  "devDependencies": {
    "asar": "^0.14.0",
    "electron-packager": "^9.1.0",
    "pathfinding": "^0.4.18",
    "ramda": "^0.25.0",
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-heat": "^0.4.0",
    "sagui": "^11.4.2"
  },
  "dependencies": {
    "@most/create": "^2.0.1",
    "electron": "^1.7.9",
    "most": "^1.7.2",
    "ssb-client": "^4.5.1",
    "ssb-party": "^0.5.1"
  }
